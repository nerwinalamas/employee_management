generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Employee {
  id        String   @id @default(cuid()) @map("_id")
  name      String
  email     String
  position  String
  phone     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
